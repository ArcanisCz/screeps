{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "lib": [
      "dom",
      "es2016"
    ],
    "sourceMap": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "removeComments": false,
  },
  "types": [
    "lodash",
    "screeps"
  ],
  "exclude": [
    "node_modules"
  ]
}
